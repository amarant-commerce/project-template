{
  "name": "amarant2/project-template",
  "description": "Amarant Commerce project template",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.amarant-commerce.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "amarant2/module-backoffice": "^1.0",
    "amarant2/module-cms": "^1.0",
    "amarant2/module-framework": "^1.0",
    "amarant2/module-product": "^1.0",
    "amarant2/module-sales": "^1.0",
    "amarant2/module-store": "^1.0",
    "amarant2/module-ui": "^1.0",
    "amarant2/theme-hadron": "^1.0",
    "amarant2/theme-lux": "^1.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-0": {
      "": [
        "generated/code/"
      ]
    }
  }
}
